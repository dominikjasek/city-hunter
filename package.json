{
  "name": "city-hunter",
  "version": "10.18.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3400",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "migration:generate": "npx drizzle-kit generate:mysql",
    "migration:run": "npx drizzle-kit push:mysql"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@clerk/nextjs": "^4.14.2",
    "@edge-runtime/types": "^2.0.8",
    "@emotion/cache": "^11.10.7",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.12.0",
    "@planetscale/database": "^1.6.0",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.18.0",
    "@trpc/next": "^10.18.0",
    "@trpc/react-query": "^10.18.0",
    "@trpc/server": "^10.18.0",
    "clsx": "^1.1.1",
    "drizzle-orm": "^0.23.5",
    "jose": "^4.13.1",
    "mysql2": "^3.2.0",
    "next": "^13.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.12.2",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@clerk/types": "^3.33.0",
    "@playwright/test": "^1.26.1",
    "@types/node": "^18.7.20",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dotenv": "^16.0.3",
    "drizzle-kit": "0.17.1-609a4f0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "tsx": "^3.12.3",
    "typescript": "^5.0.3",
    "vite": "^4.1.2",
    "vitest": "^0.28.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
