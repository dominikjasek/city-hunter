// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int           @id @default(autoincrement())
  thirdPartyId String
  email        String
  photoUrl     String
  firstName    String
  lastName     String
  provider     LoginProvider
}

enum LoginProvider {
  GOOGLE
}
